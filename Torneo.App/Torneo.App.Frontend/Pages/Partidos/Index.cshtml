@page
@model Torneo.App.Frontend.Pages.Partidos.IndexModel
@{
}
<h1>Listado de Partidos</h1>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Nombre</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var partido in Model.partidos)
    {
      <tr>
        <th>@partido.Id</th>
        <td>@partido.Nombre</td>
      </tr>
    }
  </tbody>
</table>
