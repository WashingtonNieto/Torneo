@page
@model Torneo.App.Frontend.Pages.Posiciones.IndexModel
@{
}
<h1>Listado de Posiciones</h1>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Nombre</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var municipio in Model.municipios)
    {
      <tr>
        <th>@municipio.Id</th>
        <td>@municipio.Nombre</td>
      </tr>
    }
  </tbody>
</table>