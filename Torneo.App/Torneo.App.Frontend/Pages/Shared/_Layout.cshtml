<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Torneo de Futball</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Torneo.App.Frontend.styles.css" asp-append-version="true" />
</head>

<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3"> 
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-page="/Index">Torneo Futball</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse"
                    aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav">

                        <div class="dropdown">
                            <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenu2"
                                data-bs-toggle="dropdown" aria-expanded="false">
                                Municipios
                            </button>
                            <ul class="dropdown-menu" aria-labelledby="dropdownMenu2">
                                <a class="dropdown-item" asp-area="" asp-page="/Municipios/Create">Crear</a>
                                <a class="dropdown-item" asp-area="" asp-page="/Municipios/Index">Mostrar</a>
                                <a class="dropdown-item" asp-area="" asp-page="/Municipios/Index">Actualizar</a>
                                <a class="dropdown-item" asp-area="" asp-page="/Municipios/Index">Borrar</a>
                            </ul>
                        </div>

                        <div class="dropdown">
                            <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenu2"
                                data-bs-toggle="dropdown" aria-expanded="false">
                                Directores Tecnicos
                            </button>
                            <ul class="dropdown-menu" aria-labelledby="dropdownMenu2">
                                <a class="nav-link text-dark" asp-area=""
                                    asp-page="/DirectoresTecnicos/Create">Crear</a>
                                <a class="nav-link text-dark" asp-area=""
                                    asp-page="/DirectoresTecnicos/Index">Mostrar</a>
                                <li><button class="dropdown-item" type="button">Actualizar</button></li>
                                <li><button class="dropdown-item" type="button">Borrar</button></li>
                            </ul>
                        </div>

                        <div class="dropdown">
                            <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenu2"
                                data-bs-toggle="dropdown" aria-expanded="false">
                                Equipos
                            </button>
                            <ul class="dropdown-menu" aria-labelledby="dropdownMenu2">
                                <li><button class="dropdown-item" type="button">Crear</button></li>
                                <a class="nav-link text-dark" asp-area="" asp-page="/Equipos/Index">Mostrar</a>
                                <li><button class="dropdown-item" type="button">Actualizar</button></li>
                                <li><button class="dropdown-item" type="button">Borrar</button></li>
                            </ul>
                        </div>

                        <div class="dropdown">
                            <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenu2"
                                data-bs-toggle="dropdown" aria-expanded="false">
                                Posiciones
                            </button>
                            <ul class="dropdown-menu" aria-labelledby="dropdownMenu2">
                                <li><button class="dropdown-item" type="button">Crear</button></li>
                                <a class="nav-link text-dark" asp-area="" asp-page="/Posiciones/Index">Mostrar</a>
                                <li><button class="dropdown-item" type="button">Actualizar</button></li>
                                <li><button class="dropdown-item" type="button">Borrar</button></li>
                            </ul>
                        </div>

                        <div class="dropdown">
                            <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenu2"
                                data-bs-toggle="dropdown" aria-expanded="false">
                                Jugadores
                            </button>
                            <ul class="dropdown-menu" aria-labelledby="dropdownMenu2">
                                <li><button class="dropdown-item" type="button">Crear</button></li>
                                <li><button class="dropdown-item" type="button">Mostrar</button></li>
                                <li><button class="dropdown-item" type="button">Actualizar</button></li>
                                <li><button class="dropdown-item" type="button">Borrar</button></li>
                            </ul>
                        </div>

                        <div class="dropdown">
                            <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenu2"
                                data-bs-toggle="dropdown" aria-expanded="false">
                                Partidos
                            </button>
                            <ul class="dropdown-menu" aria-labelledby="dropdownMenu2">
                                <li><button class="dropdown-item" type="button">Crear</button></li>
                                <li><button class="dropdown-item" type="button">Mostrar</button></li>
                                <li><button class="dropdown-item" type="button">Actualizar</button></li>
                                <li><button class="dropdown-item" type="button">Borrar</button></li>
                            </ul>
                        </div>
                    </ul>
                </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - Torneo.App.Frontend - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>