@page
@model Torneo.App.Frontend.Pages.DirectoresTecnicos.IndexModel
@{
}
<h1>Listado de Directores Tecnicos</h1>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Nombre</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var directorTecnico in Model.directoresTecnicos)
    {
      <tr>
        <th>@directorTecnico.Id</th>
        <td>@directorTecnico.Nombre</td>
      </tr>
    }
  </tbody>
</table>