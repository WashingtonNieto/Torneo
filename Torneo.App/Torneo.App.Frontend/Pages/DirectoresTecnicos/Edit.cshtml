@page
@model Torneo.App.Frontend.Pages.DirectoresTecnicos.EditModel
@{
}
<h1>Editar Director Tecnico</h1>
<form method="post">
  <div class="form-group">
    <label asp-for="dt.Id"></label>
    <input asp-for="dt.Id" class="form-control" readonly>
  </div>
  <div class="form-group">
    <label asp-for="dt.Nombre"></label>
    <input asp-for="dt.Nombre" class="form-control">
    <span class="text-danger" asp-validation-for="dt.Nombre"></span>
  </div>
  <div class="form-group">
    <label asp-for="dt.Documento"></label>
    <input asp-for="dt.Documento" class="form-control">
    <span class="text-danger" asp-validation-for="dt.Documento"></span>
  </div>
  <div class="form-group">
    <label asp-for="dt.Telefono"></label>
    <input asp-for="dt.Telefono" class="form-control">
    <span class="text-danger" asp-validation-for="dt.Telefono"></span>
  </div>
  <button type="submit" class="btn btn-primary btn-block">Actualizar</button>
</form>
<a class="btn btn-secondary" asp-page="./Index">Volver</a>