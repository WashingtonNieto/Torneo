@page
@model Torneo.App.Frontend.Pages.Equipos.IndexModel
@{
}
<h1>Listado de Equipos</h1>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Nombre</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var equipo in Model.equipos)
    {
      <tr>
        <th>@equipo.Id</th>
        <td>@equipo.Nombre</td>
      </tr>
    }
  </tbody>
</table>